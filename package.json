{
  "name": "hans",
  "version": "1.0.1",
  "description": "An open-source Discord bot built with Discord.JS & TypeScript",
  "main": "src/index.js",
  "scripts": {
    "start": "node build/utils/deploy-commands.js && node build/index.js",
    "dev": "ISDEV=true nodemon hans",
    "build": "rm -rf ./build && tsc --project ./",
    "lint": "eslint . --fix",
    "format": "npx prettier --write '**/*.ts'",
    "prepare": "npx husky install",
    "slash": "export ISDEV=false && npm run build && node build/utils/deploy-commands.js",
    "slashDev": "export ISDEV=true && npm run build && node build/utils/deploy-commands.js",
    "test": "ts-mocha -p ./tsconfig.json ./tests/**/*.spec.ts"
  },
  "author": "en3sis",
  "repository": {
    "type": "git",
    "url": "https://github.com/en3sis/hans"
  },
  "license": "MIT",
  "devDependencies": {
    "@discordjs/rest": "^1.1.0",
    "@typegoose/typegoose": "^9.11.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "@types/sentiment": "^5.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node-cron": "^3.0.4",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^4.9.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "openai": "^3.0.0",
    "sentiment": "^5.0.2",
    "socket.io-client": "^4.5.1"
  }
}
